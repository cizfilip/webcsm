<configuration>
  <connectionStrings>
    <add name="ApplicationServices" connectionString="data source=.\SQLEXPRESS;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|webcsm.mdf;User Instance=true" providerName="System.Data.SqlClient" />
  
  
  <add name="webcsmEntities" connectionString="metadata=res://*/Models.webcsm.csdl|res://*/Models.webcsm.ssdl|res://*/Models.webcsm.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\webcsm.mdf;Integrated Security=True;User Instance=True;MultipleActiveResultSets=True&quot;" providerName="System.Data.EntityClient" /></connectionStrings>

  <system.web>
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Routing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Mvc, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Data.Entity, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      </assemblies>
    </compilation>
    <authentication mode="Forms">
      <forms loginUrl="~/Account/LogOn" defaultUrl="~/Home/Index" timeout="60" />
    </authentication>
    <membership defaultProvider="webcsmMembershipProvider">
      <providers>
        <clear />
        <add name="webcsmMembershipProvider" type="Altairis.Web.Security.SimpleSqlMembershipProvider" connectionStringName="ApplicationServices" requiresUniqueEmail="true" />
      </providers>
    </membership>
    <profile enabled="false" automaticSaveEnabled="false" defaultProvider="webcsmProfileProvider">
      <providers>
        <clear />
        <!--<add name="webcsmProfileProvider"
             type="Altairis.Web.Security.SimpleSqlProfileProvider, Altairis.Web.Security"
             connectionStringName="ApplicationServices"
             tableName="Profiles"
             keyColumnName="UserName"
             lastUpdateColumnName="LastUpdate" />-->
      </providers>
      <properties>
        <!--<add name="FullName" type="String" customProviderData="FullName;varchar;50" />
        <add name="BirthDate" type="DateTime" defaultValue="01.01.1900" customProviderData="BirthDate;smalldatetime" />
        <add name="PageSize" type="Int32" defaultValue="10" customProviderData="PageSize;int"/>-->
      </properties>
    </profile>
    <roleManager enabled="true" defaultProvider="webcsmRoleProvider">
      <providers>
        <clear />
        <add name="webcsmRoleProvider" type="Altairis.Web.Security.SimpleSqlRoleProvider, Altairis.Web.Security" connectionStringName="ApplicationServices" />
      </providers>
    </roleManager>
    <pages>
      <namespaces>
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="webcsm.Helpers" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true" />
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0" newVersion="2.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>